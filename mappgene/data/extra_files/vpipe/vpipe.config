[input]
samples_file = samples.tsv
reference = references/NC_045512.2.fasta
fastq_suffix =
trim_percent_cutoff = 0.5

[output]
snv = True
local = False
global = False

[general]
aligner = bwa
snv_caller = lofreq

[bwa_align]
mem=2048
threads=24

[snv]
consensus=false

[lofreq]
consensus=false

[preprocessing]
extra = -ns_max_n 4 -min_qual_mean 20 -trim_qual_left 20 -trim_qual_right 20 -trim_qual_window 10
